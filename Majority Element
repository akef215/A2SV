class Solution(object):
    def majorityElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        freq = dict()
        for i in nums:
            freq[i] = freq.setdefault(i, 0) + 1
        for i in freq:
            if freq[i] > len(nums) // 2:
                return i     
        
