class Solution(object):
    def transpose(self, matrix):
        """
        :type matrix: List[List[int]]
        :rtype: List[List[int]]
        """
        r, c = len(matrix), len(matrix[0])
        res = [[0] * r for _ in range(c)]
        for i in range(c):
            for j in range(r):
                res[i][j] = matrix[j][i]
        return res
        
