class Solution(object):
    def minMovesToSeat(self, seats, students):
        """
        :type seats: List[int]
        :type students: List[int]
        :rtype: int
        """
        seats.sort(reverse=True)
        students.sort(reverse=True)
        ans = 0
        for i in range(len(seats)):
            ans += abs(students[i] - seats[i])
        return ans    
